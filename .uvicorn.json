{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(message)s"
    }
  },
  "handlers": {
    "null": {
      "class": "logging.NullHandler"
    },
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": ["null"],
      "level": "WARNING",
      "propagate": false
    },
    "uvicorn.error": {
      "handlers": ["console"],
      "level": "ERROR",
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": ["null"],
      "level": "CRITICAL",
      "propagate": false
    },
    "uvicorn.server": {
      "handlers": ["null"],
      "level": "CRITICAL",
      "propagate": false
    }
  }
}
